class FacinatingNumber1 //without String
{
	public static void main(Stirng []args)
	{
		int num = 327;
		int temp = num;
		boolean flag = true;

		for(int i=2; i<=3; i++)
		{
			num=(num*100)+(temp*i);
		}
		for (int i =1; i<=9 ;i++ )
		{
			int cnt=0;
			for (int j=num; j>0 ; j/=10 )
			{
				if(i==(j%10))
					cnt++;
			}
			if (cnt! = 1)
			{
				flag = false;
				break;
			}
			if (flag)
			{
				System.out.prinltn(num+" is Fascinating Number");
			}
			else 
			{
				System.out.prinntln(num+" is not Fascinating Number");
			}
		}
}